          "background": "#3B5AC4",
          "transition": "0.3s"
        }
      },
      "cta": {
        "default": {
          "background": "#FFFFFF",
          "color": "#121B4A"
        },
        "hover": {
          "transform": "scale(1.05)",
          "boxShadow": "0 4px 16px rgba(0, 0, 0, 0.2)",
          "transition": "0.3s"
        }
      }
    },
    "cards": {
      "default": {
        "boxShadow": "0 4px 12px rgba(0, 0, 0, 0.1)"
      },
      "hover": {
        "boxShadow": "0 8px 24px rgba(0, 0, 0, 0.15)",
        "transform": "translateY(-4px)",
        "transition": "0.3s"
      }
    },
    "diagramNodes": {
      "default": {
        "opacity": "1",
        "scale": "1"
      },
      "hover": {
        "scale": "1.1",
        "background": "rgba(255, 255, 255, 0.15)",
        "transition": "0.3s"
      }
    },
    "logos": {
      "default": {
        "opacity": "0.7",
        "filter": "grayscale(100%)"
      },
      "hover": {
        "opacity": "1",
        "filter": "grayscale(0%)",
        "transition": "0.3s"
      }
    }
  }
}
